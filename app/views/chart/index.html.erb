<%= @result.inspect %>
<script type="text/javascript">
	var dataset = [5, 10, 15, 20, 25];
	d3.select('body').selectAll('p')
		.data(dataset)
		.enter()
		.append('p')
		.text(function(d){
			return "Yo " + d + " times."
		})
		.style("color", function(d) {
		    if (d > 15) {   //Threshold of 15
		        return "red";
		    } else {
		        return "black";
		    }
		});      

	d3.select('body').selectAll('div')
		.data(dataset)
		.enter()
		.append('div')
		.classed('bar', true)
		.style('height', function(d){return (d * 5) + "px";});
</script>